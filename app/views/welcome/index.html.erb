<h1>Viewing Party</h1>
<br>
<% if current_user %>
  <div id="new-user">
    <%= link_to 'Log Out', logout_path, method: :get %>
    <%= link_to 'My Dashboard', user_path(current_user), method: :get %>
    <div id="existing-users">
      <h2>Existing Users</h2>
      <% @users.each do |user| %>
        <p><%= user.email  %></p>
      <% end %>
    </div>
  <% else %>
    <%= button_to 'New User', register_path, method: :get %>
    <%= link_to 'Log In', login_path, method: :get %>
</div>
<% end %>
<br>