<h1>Viewing Party</h1>
<br>
<h2>Log in</h2>
<div id="login-form">
  <%= form_with url: login_path, method: :post do |form| %>
    <%= form.label :email %>
    <%= form.text_field :email %>

    <%= form.label :password, 'Password' %>
    <%= form.password_field :password %>

    <%= form.submit 'Log In' %>
  <% end %>
</div>